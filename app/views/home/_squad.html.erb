<div class="page-header">
  <h2><%= active_squad.name %> - <%= active_squad.positive_percentage %>%</h2>
  <table class="table table-striped">
  	<tr>
  		<th>#</th>
  		<th>Nome</th>
  		<th>Tweets positivos</th>
  	</tr>
  	<% @players.each do |player| %>
  	<tr>
  		<td><%= player.id %></td>
  		<td><%= link_to player.name %></td>
  		<td><span class="badge"><%= player.positive_percentage %>%</span></td>
  	</tr>
  	<% end %>
  </table>
</div>